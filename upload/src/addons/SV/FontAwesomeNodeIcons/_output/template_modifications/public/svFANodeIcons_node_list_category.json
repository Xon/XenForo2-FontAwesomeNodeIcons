{
    "template": "node_list_category",
    "description": "Inject FA node icons",
    "execution_order": 1,
    "enabled": true,
    "action": "preg_replace",
    "find": "#(<span class=\"node-icon\" aria-hidden=\"true\">).*?(</span>)#si",
    "replace": "$1<xf:fa icon=\"{{ $node.getFontAwesomeIcon($extras) }}\" />$2"
}